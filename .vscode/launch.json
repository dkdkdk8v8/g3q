{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "node:admin",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "cwd": "${workspaceFolder}/admin",
            "console": "integratedTerminal",
            "restart": true,
            "autoAttachChildProcesses": true
        },
        {
            "name": "node:admin_client",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "cwd": "${workspaceFolder}/admin_client",
            "console": "integratedTerminal",
            "restart": true,
            "autoAttachChildProcesses": true
        },
        {
            "name": "go:server:client",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/server/server/main_client", // 调试整个工作区（入口是 main 包）
            "output": "${workspaceFolder}/server/server/main_client/local_main_client",
            "args": [
                "--ter",
                "--debug"
            ], // 传递给程序的命令行参数
            "env": {
                "test": "test" // 设置临时环境变量
            }
        },
        {
            "name": "go:server:robot",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/server/server/main_robot", // 调试整个工作区（入口是 main 包）
            "output": "${workspaceFolder}/server/server/main_robot/local_main_robot",
            "args": [
                "--ter",
                "--debug"
            ], // 传递给程序的命令行参数
            "env": {
               
            }
        },
        {
            "name": "go:file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}", // 调试当前编辑器打开的那个文件
            "showLog": true,
            "output": "${workspaceFolder}/server/ztest/zbinary"
        },
        {
            "name": "go:server:remote_client",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/server/server/main_client", // 调试整个工作区（入口是 main 包）
            "output": "${workspaceFolder}/server/server/main_client/remote_main_client",
            "args": [
                "--ter"
            ], // 传递给程序的命令行参数
            "env": {
                "test": "test" // 设置临时环境变量
            },
            "preLaunchTask": "SSH_Tunnel_Task",
            "showLog": true
        },
        {
            "name": "node:client",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "cwd": "${workspaceFolder}/client",
            "console": "integratedTerminal",
            "restart": true,
            "autoAttachChildProcesses": true
        },
    ],
    "compounds": [
        {
            "name": "Launch All Admin",
            "configurations": [
                "node:admin",
                "node:admin_client"
            ],
            "stopAll": true
        }
    ]
}